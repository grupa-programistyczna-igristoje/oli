#include "stdafx.h" //nie wiem po jaki huj to//
#include <iostream> //standardowo//
#include <math.h> //dzialania//
#include <cstdlib> //do systemu//

using namespace std;
int main()
{
	int a,b,c,del;
cout << " \t nie umiem into wizual \n";
cout << " \t ale zrobilem kalkulator delty \n";
cout << " \t no to podaj najpierw a \n";
cin >> a;
cout << " \t to teraz podaj b \n";
cin >> b;
cout << " \t to teraz podaj c \n";
cin >> c;
if (a > 0 || b > 0 || c > 0) 
{
	del:((b*b) - (4 *a*c));
	cout << "\t delta wynosi:",del;
	return 0;
}
else{
	cout << "cos spierdoliles";
}
return 0;
}
