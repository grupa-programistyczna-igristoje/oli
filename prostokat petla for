     int main(int argc, char *argv[]) // no i po chuj ci te argc i argv co
     {
    int w,s;
    char x;
    cout<<"\t \t super program rysujacy prostokat z gwiazdeczek avira plz"<<endl;
    cout<<"\t \t podaj wysokosc \n"<<endl;
    cin>>w;
    cout<<"\t \t podaj szerokosc \n"<<endl;
    cin>>s;

       for (int i=1; i<=w; i++){
             for (int j=1; j<=s; j++){ //(elvis)a co jeśli wysokość będzie mniejsza od szerokości? edit: już nic
             cout << "*";
    			  }

    		 cout<<"\n";
             }
